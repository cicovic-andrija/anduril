{
    "https": {
        "network": {
            "ip_accept_host": "localhost",
            "tcp_port": 8080,
            "tls_cert_path": "certfile.crt",
            "tls_key_path": "keyfile.key"
        },
        "enable_file_server": true,
        "file_server": {
            "directory": "static",
            "url_prefix": "/static/",
            "allowed": [
                "frontendProcessing.js",
                "stylesheet.css",
                "bootstrap.bundle.min.js",
                "bootstrap.min.css"
            ]
        },
        "log_requests": true,
        "allow_only_get_requests": true
    },
    "repository": {
        "url": "https://github.com/cicovic-andrija/notes.git",
        "remote": "origin",
        "branch": "master",
        "relative_content_path": "notes"
    }
}
