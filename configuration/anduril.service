[Unit]
Description=Notes Web Server
After=network.target

[Service]
Type=simple
Restart=always
User=root
WorkingDirectory=/srv/anduril
ExecStart=/srv/anduril/anduril-server --encrypted --config /srv/anduril/data/encrypted-config.txt
StandardOutput=syslog
StandardError=syslog

[Install]
WantedBy=multi-user.target
