{
    "https": {
        "network": {
            "ip_accept_host": "localhost",
            "tcp_port": 8080,
            "tls_cert_path": "tlspublic.crt",
            "tls_key_path": "tlsprivate.key"
        },
        "enable_file_server": true,
        "file_server": {
            "directory": "static",
            "url_prefix": "/static/",
            "allowed": [
                "frontendProcessing.js",
                "stylesheet.css",
                "bootstrap.bundle.min.js",
                "bootstrap.min.css"
            ]
        },
        "log_requests": true,
        "allow_only_get_requests": true
    },
    "repository": {
        "protocol": "ssh",
        "host": "github.com",
        "repo_url_path": "/cicovic-andrija/notes.git",
        "remote": "origin",
        "branch": "master",
        "relative_content_path": "notes",
        "ssh_auth": {
            "user": "git",
            "private_key_path": "gitprivate.key"
        }
    },
    "settings": {
        "publish_private_articles": false,
        "publish_personal_articles": true,
        "repository_sync_period": "15s",
        "stale_file_cleanup_period": "1h"
    }
}
